{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote",
    "packageManager":"pnpm"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      },
  "android": {
    "image": "latest"
  },
      "ios": {
        "simulator": true
      }
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "preview"
      },
      "ios": {
        "simulator": true
      }, 
  "android": {
    "image": "latest"
  }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
  
}
